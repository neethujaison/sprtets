<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.myspringpacks.model">
	<class name="UploadFile" table="tbl_uploadfile">
		<id name="file_id" column="file_id">
			<generator class="native"/>
		</id>
		<property name="file_name" column="file_name">
		</property>
		
		<property name="file_data" column="file_data">
		</property>
		
		
	</class>
	
</hibernate-mapping>